name: "去除订阅ua"
http:
  script:
    - match: ^https?://url\.v1\.mk
      name: remove_ua_for_v1mk
      type: request
      require-body: false
      timeout: 10

script-providers:
  remove_ua_for_v1mk:
    url: https://raw.githubusercontent.com/chensongxian/stash_config/v1.0.0/remove_ua_for_v1mk.js
    interval: 86400
